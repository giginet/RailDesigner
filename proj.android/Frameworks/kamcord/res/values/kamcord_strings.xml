    <style name="KamcordButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:gravity">center</item>
        <item name="android:minHeight">@null</item>
        <item name="android:minWidth">@null</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:paddingRight">30dp</item>
        <item name="android:background">@drawable/kamcord_button_background</item>
        <item name="android:textColor">@color/kamcordButtonTextColor</item>
        <item name="android:textSize">20dp</item>
    </style>
    
    <style name="KamcordButton.Gray">
        <item name="android:background">@drawable/kamcord_button_background_gray</item>
    </style>
    
    <style name="KamcordButton.Share">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:padding">0dp</item>
        <item name="android:textSize">25dp</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="KamcordButton.Share.Landscape">
        <item name="android:layout_height">76.333dp</item>
    </style>

    <style name="KamcordButton.Small">
        <item name="android:layout_width">90dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">0dp</item>
        <item name="android:background">@drawable/kamcord_toggle_selector</item>
        <item name="android:textSize">17dp</item>
    </style>

    <style name="KamcordEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">10dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:fontFamily">sans-serif</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:background">@null</item>
    </style>

